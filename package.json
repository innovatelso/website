    "main": "index.js",
    "scripts": {
        "dev": "NODE_ENV=development concurrently \"npm run dev:build-client\" \"npm run dev:build-server\" \"npm run dev:server\"",
        "dev:server": "nodemon --watch --ignore src/client/ build --exec \"node build/bundle.js\"",
        "dev:build-server": "webpack --config config/webpack.server.js --watch",
        "dev:build-client": "webpack --config config/webpack.client.dev.js",
        "build:client": "NODE_ENV=production webpack --config config/webpack.client.production.js -p",
        "build:server": "NODE_ENV=production webpack --config config/webpack.server.js -p",
        "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
        "start": "NODE_ENV=production node build/bundle.js",
        "start:server": "NODE_ENV=production pm2 start build/bundle.js --name=\"innovatelso\""
    },
